                <% const monthNames=[ "January" , "February" , "March" , "April" , "May" , "June" , "July" , "August"
                  , "September" , "October" , "November" , "December" ]; %>
                  <div class="tabs" id="monthtab">

                    <% [...new Set(trips.citydetails[0].Dates.split(",").map(date=>
                      new Date(date).getMonth()))].forEach(function(date, index) { %>
                      <% if (index==0) { %>
                        <button class="tab-button active" onclick="openMonth(event, '<%= monthNames[date] %>')">
                          <%= monthNames[date] %>
                        </button>

                        <% } else { %>
                          <button class="tab-button" onclick="openMonth(event, '<%= monthNames[date] %>')">
                            <%= monthNames[date] %>
                          </button>
